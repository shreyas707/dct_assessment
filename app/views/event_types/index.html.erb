<p id="notice"><%= notice %></p>

<h1>Listing Event Types</h1>
<%= link_to 'New Event type', new_event_type_path, class: "ui primary button" %><br/><br/>

<table class="ui celled padded table dataTable">
  <thead>
    <tr>
      <th>Name</th>
      <th>Show</th>
    </tr>
  </thead>

  <tbody>
    <% @event_types.each do |event_type| %>
      <tr>
        <td><%= event_type.name %></td>
        <td><%= link_to 'Show', event_type %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br><br>